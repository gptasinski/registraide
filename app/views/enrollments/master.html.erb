<div class="container -fluid">

  <div class="col-md-10">
    <div class="row">
      <div class="col-md-offset-9 col-md-3">
        <%= render '/shared/enrollments_as_csv' %>
      </div>
    </div>
    <div class="row">
      <div class="col-md-3 head">Section Name</div>
      <div class="col-md-1 head">Period</div>
      <div class="col-md-1 head">Student ID</div>
      <div class="col-md-2 head">Last Name</div>
      <div class="col-md-1 head">First Name</div>
      <div class="col-md-3 head">Email</div>
      <div class="col-md-1 head">Grade Level</div>
    </div>
    <% enrollment_master_order.each do |enrollment| %>
      <% if enrollment.student %>
      <div class="row master-course">
        <div class="col-md-3"><%= enrollment.section.name %></div>
        <div class="col-md-1"><%= enrollment.section.period_id %></div>

        <div class="col-md-1"><%= enrollment.student.id_number %></div>
        <div class="col-md-2"><%= enrollment.student.last_name %></div>
        <div class="col-md-1"><%= enrollment.student.first_name %></div>
        <div class="col-md-3"><%= enrollment.student.email %></div>
        <div class="col-md-1"><%= enrollment.student.grade_level %></div>
      </div>
      <% end %>
    <% end %>

  </div>
</div>