  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-2 head">LAST</div>
    <div class="col-md-1 head">FIRST</div>
    <div class="col-md-2 head">EMAIL</div>
    <div class="col-md-1 head">ID#</div>
    <div class="col-md-2 head">HOMEROOM</div>
    <div class="col-md-1 head">GRADE LEVEL</div>
  </div>

  <% student_order(@students).each do |student| %>
    <div class="row output">
      <div class="col-md-1"><%= student_index(@students, student) %></div>
      <div class="col-md-2"><%= student.last_name %></div>
      <div class="col-md-1"><%= student.first_name %></div>
      <div class="col-md-2"><%= student.email %></div>
      <div class="col-md-1"><%= student.id_number %></div>
      <div class="col-md-2"><%= student.current_homeroom %></div>
      <div class="col-md-1"><%= student.grade_level %></div>
      <div class="col-md-1">
        <%= link_to 'Schedule', user_path(student), class: "btn btn-default btn-sm my-btn" %>
      </div>
      <div class="col-md-1">
        <% if student.schedule_confirmed %>
          CONFIRMED
        <% end %>
      </div>
    </div>
  <% end %>