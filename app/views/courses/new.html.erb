<div class="container">
  <div class="col-md-10 new-form-box">
    <div class="row">
      <%= form_for @course, class: "form-horizontal" do |form| %>
      <%= render partial:'form', locals: { form: form } %>
      <div class="form-group">
        <div class="col-md-offset-10 col-md-2">
          <%= form.submit :'Add Course', class: "btn btn-default" %>
        </div>
      </div>
      <% end %>
    </div>
  </div>
</div>